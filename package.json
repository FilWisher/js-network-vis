{
  "name": "nviz",
  "version": "0.0.1",
  "description": "visualization of a network",
  "main": "index.js",
  "scripts": {
    "test": "tape test/test.js",
    "build": "mkdir -p dist && browserify examples/index.js > examples/bundle.js",
    "build:w": "mkdir -p dist && watchify examples/index.js -o examples/bundle.js -v",
    "cover": "istanbul cover tape test/test.js",
    "serve": "live-server examples/",
    "build:wb": "npm run build:w &",
    "dev": "npm run build:wb && npm run serve",
    "tree": "tree -I 'node_modules|coverage'"
  },
  "author": "FilWisher",
  "license": "MIT",
  "devDependencies": {
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "istanbul": "^0.4.2",
    "jsdom": "^8.2.0",
    "live-server": "^0.9.2",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "d3": "^3.5.16"
  }
}
